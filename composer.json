{
  "name": "bavix/laravel-helpers",
  "description": "Language Helpers for Laravel 5",
  "license": "MIT",
  "keywords": [
    "laravel",
    "language",
    "helpers"
  ],
  "authors": [
    {
      "name": "Babichev Maxim",
      "email": "info@babichev.net"
    }
  ],
  "require": {
    "php": ">=7.0",
    "laravel/framework": "^5.5",
    "encore/laravel-admin": "^1.5",
    "spatie/laravel-tags": "^2.0",
    "bavix/slice": "^1.0",
    "bavix/sdk": "^1.0"
  },
  "require-dev": {
    "intervention/image": "^2.4"
  },
  "autoload": {
    "files": [
      "helpers/functions.php",
      "helpers/cookies.php",
      "helpers/uploads.php"
    ],
    "psr-4": {
      "Bavix\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Bavix\\Providers\\AdminServiceProvider"
      ]
    }
  }
}
